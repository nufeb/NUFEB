FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          controlDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     hybridFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         30;

deltaT          0.001;

writeControl    runTime;

writeInterval   0.5;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

functions
{
    probes
    {
        type                probes;
        functionObjectLibs  ("libsampling.so");
        outputControl       timeStep;
        outputInterval      1;
        probeLocations
        (
            (0.02 0.002 0.003) 
            (0.02 0.16 0.003) 
            (0.01 0.002 0.003) 
            (0.01 0.16 0.003) 
            (0.002 0.002 0.003) 
            (0.002 0.16 0.003) 
            (0.002 0.04 0.003) 
            (0.038 0.002 0.003) 
            (0.038 0.04 0.003) 
            (0.03 0.002 0.003) 
            (0.03 0.04 0.003) 
        );
        fields
        (
            p
            Ub
        );
    }

}


