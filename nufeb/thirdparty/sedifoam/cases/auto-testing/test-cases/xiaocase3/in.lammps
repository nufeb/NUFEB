# 2D particle pipe simulation

atom_style	sphere
atom_modify	map array
boundary	ff ff ff
newton		off
# processors  1 2 1

communicate single vel yes
read_data 	IC_uniform.in

neighbor	5.0e-4 bin
neigh_modify	delay 0

# pair_style  none
pair_style  gran/hooke/history 5000.0 NULL 11200 NULL 0.1 0
pair_coeff  * *
timestep	2e-7

velocity    all set 0.0 0.0 0.0 units box

fix		1 all nve/sphere  
fix		2 all gravity 0.0 vector 0 -1 0 # spherical 90.0 -180.0    
fix     3 all fdrag

fix  xwall all wall/gran 5000.0 NULL 11200 NULL 0.1 0 xplane 0.00 0.004
fix  ywall all wall/gran 5000.0 NULL 11200 NULL 0.1 0 yplane 0.00 0.004
fix  zwall all wall/gran 5000.0 NULL 11200 NULL 0.1 0 zplane 0.00 0.0005

#dump		id all custom 30000 snapshot2.bubblemd tag type x y z vx vy vz
dump		id all custom 1000 snapshot.bubblemd id type diameter mass x y z vx vy vz

#thermo_style  granular # not work. syntax change?
thermo_style  one   # granular does not work
thermo		2000
thermo_modify	lost error
#restart		1000000  restart.*.bubblemd


