    lineY1
    {
        type    midPointAndFace;
        axis    x;

        start   (0.001   0.0164  0.005);
        end     (0.043   0.0164  0.005);
        nPoints 100;
    }

    lineY2
    {
        type    midPointAndFace;
        axis    x;

        start   (0.001   0.0312  0.005);
        end     (0.043   0.0312  0.005);
        nPoints 100;
    }

    lineY3
    {
        type    midPointAndFace;
        axis    x;

        start   (0.001   0.0199  0.005);
        end     (0.043   0.0199  0.005);
        nPoints 100;
    }
);

