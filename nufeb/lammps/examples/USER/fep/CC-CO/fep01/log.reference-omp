  9 = max # of special neighbors
# read_restart restart.*.lmp
# reset_timestep 0

pair_style hybrid lj/cut/coul/long 10.0 10.0   lj/cut/coul/long/soft 1 0.5 10.0 10.0 10.0
pair_modify tail no
kspace_style pppm 1.0e-4

